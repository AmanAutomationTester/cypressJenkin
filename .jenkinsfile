pipeline
{
    agent any

    stages
    {
        stage('Build')
        {
            steps
            {
                bat "npm install"
            }
        }
        stage ('Test')
        {
            steps
            {
                echo 'Executing spec file in Cypress'
                bat "npm test"
            }
        }
        stage ('Read failures json')
        {
            steps
            {
                $files = Get - ChildItem - Path /cypress/logs - Filter *.json
                foreach ($file in $files) {
                    $content = Get - Content $file.FullName - Raw | ConvertFrom 
                        - Json
                    # Do something with $content
                }
            }
        }
    }
}